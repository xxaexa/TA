<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>eXa-Voucher</title>
    <link rel="stylesheet" href="./src/css/style.css" />
  </head>
  <body class="bg-gradient-to-t from-main to-white">
    <!-- header -->
    <header class="bg-white absolute top-0 left-0 w-full flex items-center z-10">
      <div class="container">
        <div class="flex items-center lg:justify-center relative">
          <div class="px-4">
            <a href="#home" class="font-bold text-lg block py-6"><img src="./src/img/exa.png" alt="exaLogo" class="w-10 lg:hidden" /></a>
          </div>
          <div class="flex items-center px-4">
            <button id="hamburger" name="hamburger" type="button" class="block absolute right-4 lg:hidden px-4">
              <span class="hamburger-line transition duration-300 ease-in-out origin-top-left"></span>
              <span class="hamburger-line transition duration-300 ease-in-out"></span>
              <span class="hamburger-line transition duration-300 ease-in-out origin-bottom-left"></span>
            </button>
            <!-- nav -->
            <nav id="nav-menu" class="hidden absolute py-5 bg-white shadow-lg rounded-lg max-w-[250px] top-full lg:block lg:static lg:bg-transparent lg:max-w-full lg:shadow-none lg:rounded-none lg:justify-center">
              <ul class="block lg:flex">
                <li class="group">
                  <a href="" class="text-dark text-base py-2 mx-8 flex group-hover:text-second">HOME</a>
                </li>
                <li class="group">
                  <a href="" class="text-dark text-base py-2 mx-8 flex group-hover:text-second">TRACK</a>
                </li>
              </ul>
            </nav>
          </div>
        </div>
      </div>
    </header>
    <!-- motto -->
    <section class="pt-16">
      <div class="container">
        <div class="flex flex-wrap justify-center">
          <div class="w-full self-center px-4">
            <h1 class="text-second text-center block my-5 font-bold text-4xl md:text-6xl">CEPAT MURAH AUTO HOKI PAS GACHA</h1>
          </div>
        </div>
      </div>
    </section>
    <!-- Banner -->
    <section id="about" class="pt-5 pb-20">
      <div class="container">
        <div class="flex flex-wrap">
          <div class="w-full px-4 mb-10 uppercase text-dark">
            <img src="./src/img/bdrop.png" alt="" class="object-cover" />
          </div>
        </div>
      </div>
    </section>
    <!-- GAME -->
    <section id="game" class="pt-5 pb-5">
      <h1 class="mb-14 text-main text-center block font-bold text-4xl md:text-6xl">GAME</h1>

      <div class="container">
        <div class="flex flex-wrap">
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/valo.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/gi.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/ff.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/mlbb.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/pubg.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
          <div class="w-1/2 px-4 mb-10 uppercase text-dark md:w-1/3">
            <a href=""><img src="./src/img/aov.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
          </div>
        </div>
      </div>
    </section>
    <!-- MIX -->
    <section id="mix" class="pt-5 pb-5">
      <div class="container">
        <div class="flex flex-wrap">
          <div class="w-1/2 px-4 mb-10">
            <h1 class="mb-14 text-main text-center block font-bold text-4xl md:text-6xl">EMONEY</h1>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/gopay.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/linkaja.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/ovo.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/dana.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
          </div>
          <div class="w-1/2 px-4 mb-10">
            <h1 class="mb-14 text-main block font-bold text-4xl md:text-6xl text-center">PULSA</h1>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/isat.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/tree.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/tsel.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
            <div class="px-4 mb-10">
              <a href=""><img src="./src/img/xl.png" alt="" class="w-52 mx-auto text-center p-5 rounded-2xl shadow-xl shadow-main/50" /></a>
            </div>
          </div>
        </div>
      </div>
    </section>
    <div id="wa" class="fixed bottom-20 right-5 w-14 md:w-16 lg:w-20 bg-green-600 bg-opacity-50 rounded-full">
      <a href=""><img src="./src/img/wa.png" alt="" class="justify-end" /></a>
    </div>
    <hr class="bg-violet-400" />
    <footer class="">
      <p class="my-5 text-2xl text-white text-center"><a href="" class="hover:text-fuchsia-300 transition duration-500 ease-in-out">xxaexa</a></p>
    </footer>
    <script src="./src/js/app.js"></script>
  </body>
</html>
